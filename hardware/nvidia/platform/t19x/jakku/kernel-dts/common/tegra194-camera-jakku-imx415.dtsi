			vcc-supply = <&p3509_vdd_3v3_cvb>;
			skip_mux_detect;	
			force_bus_start = <CAMERA_I2C_MUX_BUS(0)>;			
			i2c@0 {
				status = "okay";
				reg = <0>;
				i2c-mux,deselect-on-exit;
				#address-cells = <1>;
				#size-cells = <0>;

				tca6424_22_a: tca6424@22 {
			    	compatible = "ti,tca6424";
			    	gpio-controller;
			    	#gpio-cells = <2>;
			    	reg = <0x22>;
					vcc-supply = <&p3509_vdd_sys_en>;
