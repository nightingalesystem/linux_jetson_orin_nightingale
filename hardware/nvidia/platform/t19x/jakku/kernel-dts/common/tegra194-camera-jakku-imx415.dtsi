			    	        status = "disabled";
			    	};
			    	tca6424_22_input {
			    	        status = "disabled";
			    	};

				};
				imx415_a@42 {
					reset-gpios = <&tca6424_22_a 1 GPIO_ACTIVE_HIGH>;
					boot-gpios  = <&tca6424_22_a 2 GPIO_ACTIVE_HIGH>;
