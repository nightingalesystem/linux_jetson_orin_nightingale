					devname = "e-con_cam 31-0042";
					proc-device-tree = "/proc/device-tree/i2c@3180000/tca9548@70/i2c@1/imx415_c@42";
				};
			};
			cam_module2: module2 {
				badge = "imx415_bottomleft_0415";
				position = "bottomleft";
				orientation = "1";
				status="okay";
				cam_module2_drivernode0: drivernode0 {
